{
	"compilerOptions": {
		"declaration": true,
		"target": "es2020",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"allowJs": false,
		"skipDefaultLibCheck": true,
		"allowUnreachableCode": true,
		"rootDir": ".",
		"skipLibCheck": true
	},
	"include": ["leetcode", "types", "jest", "test"],
	"exclude": ["node_modules", "lib"]
}

